-
  type: <%= provider %>
  url: <%= url %>
<% if @projects -%>
  projects:
  <%- projects.each do |project| -%>
    - <%= project.chomp %>
  <%- end -%>
<% end -%>
<% if @ignored_projects -%>
  ignored_projects:
  <%- ignored_projects.each do |project| -%>
    - <%= project.chomp %>
  <%- end -%>
<% end -%>

